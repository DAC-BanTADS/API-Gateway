{
    "compileOnSave": true,
    "compilerOptions": {
        "module": "CommonJS",
        "outDir": "./built",
        "allowJs": true,
        "target": "es5",
        "allowUnreachableCode": false,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "typeRoots": ["./typings"]
    },
    "include": ["./**/*"],
    "exclude": ["./plugins/**/*", "./typings/**/*", "./built/**/*"]
}
